{
  "name": "@ddd-tool/domain-designer-cli",
  "version": "0.2.0-aplha.0",
  "private": false,
  "description": "",
  "type": "module",
  "bin": {
    "domain-designer-cli": "./bin/domain-designer-cli.cjs"
  },
  "files": [
    "./bin",
    "./packages/playground",
    "./scripts",
    "./templates",
    "./tsconfig.json"
  ],
  "scripts": {
    "dev": "pnpm -F playground dev",
    "dev:ai": "pnpm -F ai dev",
    "dev:ui": "pnpm -F @ddd-tool/domain-designer-ui-component dev",
    "build": "pnpm syncver && pnpm -r build",
    "test": "pnpm -r test",
    "test:cli": "pnpm -F cli test",
    "verify": "pnpm -r verify",
    "syncver": "zx ./scripts/sync-version.mjs",
    "demo:run": "node ./bin/domain-designer-cli.cjs run --source=./demo --debug",
    "demo:init": "node ./bin/domain-designer-cli.cjs init --source=./demo --debug",
    "demo:runWeb": "node ./bin/domain-designer-cli.cjs runWeb --source=./demo --debug",
    "demo:update": "node ./bin/domain-designer-cli.cjs update --source=./demo --debug",
    "demo:info": "node ./bin/domain-designer-cli.cjs info --debug",
    "demo:genCode": "node ./bin/domain-designer-cli.cjs genCode --source=./demo --debug"
  },
  "keywords": [
    "domain-designer",
    "ddd"
  ],
  "author": {
    "name": "AlphaFoxz@github.com",
    "email": "841958335@qq.com"
  },
  "repository": "https://github.com/ddd-tool/domain-designer-cli-node",
  "license": "Apache-2.0",
  "devDependencies": {
    "@primeuix/themes": "^1.2.5",
    "@tsconfig/node22": "^22.0.5",
    "@types/bun": "^1.3.5",
    "@types/node": "~22.14.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/reactivity": "^3.5.26",
    "@vue/tsconfig": "^0.8.1",
    "esbuild": "^0.24.2",
    "npm-run-all2": "^8.0.4",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.26",
    "vue-tsc": "^2.2.12",
    "zx": "^8.8.5"
  },
  "dependencies": {
    "vue-fn": "0.3.0-rc"
  }
}
