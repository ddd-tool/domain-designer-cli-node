  "devDependencies": {
    "@ddd-tool/domain-designer-core": "0.1.0-beta.10",
    "@ddd-tool/domain-designer-generator": "0.1.0-beta.12",
    "@ddd-tool/domain-designer-ui-component": "0.1.0-beta.11",
    "@primeuix/themes": "^1.1.1",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
